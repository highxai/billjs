{
	"name": "billjs",
	"description": "A typescript library for billing calculations",
	"version": "0.1.0",
	"type": "module",
	"files": [
		"dist"
	],
	"module": "./dist/index.js",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch",
		"test": "bun test",
		"test:watch": "bun test --watch"
	},
	"devDependencies": {
		"@types/bun": "^1.2.23",
		"bunup": "^0.14.11",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"license": "MIT",
	"author": "Abu Raihan",
	"repository": {
		"type": "git",
		"url": "https://github.com/aburaihan/billjs"
	}
}
